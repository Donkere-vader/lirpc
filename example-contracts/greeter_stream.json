{
  "version": "0.1.0",
  "types": {
    "GreetingRequest": {
      "type": "struct",
      "fields": {
        "name": {
          "type": "string"
        }
      }
    },
    "GreetingResponse": {
      "type": "struct",
      "fields": {
        "msg": {
          "type": "string"
        }
      }
    },
    "MyError": {
      "type": "enum",
      "variants": {
        "ServerError": {}
      }
    }
  },
  "methods": {
    "greet_stream": {
      "output": {
        "type": "custom",
        "name": "GreetingResponse"
      },
      "message": {
        "type": "custom",
        "name": "GreetingRequest"
      },
      "return_type": {
        "type": "result",
        "err_variant": {
          "type": "custom",
          "name": "MyError"
        }
      }
    }
  }
}